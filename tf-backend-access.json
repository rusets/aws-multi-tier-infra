{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3BackendBucketList",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:ListBucketVersions"
      ],
      "Resource": "arn:aws:s3:::multi-tier-demo-tfstate-097635932419-566ddc"
    },
    {
      "Sid": "S3BackendObjectsRW",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:GetObjectVersion",
        "s3:DeleteObjectVersion"
      ],
      "Resource": "arn:aws:s3:::multi-tier-demo-tfstate-097635932419-566ddc/aws-multi-tier-infra/*"
    }
  ]
}
